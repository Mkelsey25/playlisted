<li>
	<div class="flex-wrapper">
		<span id="user-item" class="flex-item">
			{{user_id}}. {{user_name}}, 
		</span>
		<span id="email-item" class="flex-item">
			{{user_email}}
		</span>
		<span id="password-item" class="flex-item">
			{{user_password}}
		</span>
		<div>
			<span id="create-date-item" class="flex-item">
				Create Date: {{created_at}}
			</span>
			<span id="modify-date-item" class="flex-item">
				Update Date: {{updated_at}}
			</span>
		</div>

		<div id="btns" class="flex-item">
			<button class="delete-user" data-id="{{this.user_id}}">Delete</button>
		</div>
	</div>
</li>
	